void setup() {

  size (1000, 1000);
  
}
  void draw() {
    background(255);
    stroke(80, 51, 226);
    square(90, 110, 558); 
    stroke(102, 115, 151);
    square(138, 164, 447);
    stroke(135, 152, 176);
    square(186, 219, 346);
    stroke(235, 182, 53);
    square(247, 293, 215);
    



















    if (frameCount == 1) {
      String filename = "output";
      save(filename + ".png");
      print("canvas saved as " + filename + ".png");
    }
  }
